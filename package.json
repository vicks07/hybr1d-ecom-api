{
  "name": "hybr1d",
  "version": "1.0.0",
  "description": "REST API for an E-Commerce Marketplace",
  "main": "src/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "NODE_ENV=production node server.js",
    "dev": "NODE_ENV=development nodemon server.js",
    "seed:dev": "npx sequelize-cli db:seed:all --env development",
    "migrate:dev": "npx sequelize db:migrate --env development",
    "migrate-undo:dev": "npx sequelize-cli db:migrate:undo --env development",
    "set-database": "SETDB=true node src/script/dbsync.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vicks07/hybr1d-ecom-api.git"
  },
  "author": "Vikram T",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vicks07/hybr1d-ecom-api/issues"
  },
  "homepage": "https://github.com/vicks07/hybr1d-ecom-api#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.31.0",
    "sequelize-auto-migrations": "^1.0.3",
    "uuid": "^9.0.0"
  }
}
